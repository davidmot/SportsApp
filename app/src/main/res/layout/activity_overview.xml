<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.davidmoz.sportsapp.OverviewActivity">

    <Button
        android:id="@+id/button9"
        android:layout_width="324dp"
        android:layout_height="31dp"
        android:layout_marginBottom="28dp"
        android:background="@android:color/holo_green_dark"
        android:text="Confirm Overview"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button8"
        android:layout_width="81dp"
        android:layout_height="34dp"
        android:text="Change"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="278dp"
        tools:layout_editor_absoluteY="281dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="241dp"
        android:layout_height="27dp"
        android:text="Selected times availabe in the week"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="288dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="241dp"
        android:layout_height="27dp"
        android:text="Selected City to play at"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="169dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="241dp"
        android:layout_height="27dp"
        android:text="Selected Sports"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="43dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="81dp"
        android:layout_height="34dp"
        android:text="Change"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="278dp"
        tools:layout_editor_absoluteY="162dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="81dp"
        android:layout_height="34dp"
        android:text="Change"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="278dp"
        tools:layout_editor_absoluteY="36dp" />

    <ScrollView
        android:layout_width="324dp"

        android:layout_height="126dp"
        android:background="?android:attr/colorBackground"
        android:clipToPadding="false"
        android:fillViewport="false"
        android:scrollbarStyle="insideInset"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="323dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
    </ScrollView>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="State"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="250dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="147dp"
        android:layout_height="21dp"
        android:background="?android:attr/colorBackground"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="249dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="147dp"
        android:layout_height="21dp"
        android:background="?android:attr/colorBackground"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="208dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="City"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="208dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:text="Settings Overview"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>
